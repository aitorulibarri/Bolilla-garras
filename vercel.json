{
    "version": 2,
    "builds": [
        {
            "src": "server.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/sw.js",
            "dest": "/public/sw.js",
            "headers": {
                "Cache-Control": "no-cache",
                "Content-Type": "application/javascript"
            }
        },
        {
            "src": "/manifest.json",
            "dest": "/public/manifest.json",
            "headers": {
                "Content-Type": "application/json"
            }
        },
        {
            "src": "/(.*)",
            "dest": "/server.js"
        }
    ]
}